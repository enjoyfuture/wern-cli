{
  "name": "wern-cli",
  "version": "0.1.1",
  "author": {
    "name": "Linder Wang"
  },
  "keywords": [
    "react",
    "router",
    "redux",
    "immutable",
    "webpack",
    "gulp"
  ],
  "description": "A Comprehensive Guide for React Boilerplate containing Webpack, Gulp, React, Redux, Router, Immutable, etc.",
  "main": "lib",
  "bin": {
    "wern": "./bin/wern.js",
    "werng": "./bin/werng.js"
  },
  "watch": {
    "compile": "src/**/*.js"
  },
  "scripts": {
    "compile": "rimraf lib && babel --presets es2015 -d lib/ src/ && sudo npm link",
    "watch": "npm-watch"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "ejs": "^2.5.2",
    "fullname": "^3.1.0",
    "glob": "^7.1.1",
    "inquirer": "^1.2.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.1",
    "path-exists": "^3.0.0",
    "path-is-absolute": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "^3.7.1",
    "eslint-plugin-flowtype": "^2.20.0",
    "eslint-plugin-react": "^6.4.0",
    "npm-watch": "^0.1.6",
    "rimraf": "^2.5.4"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enjoyfuture/wern-cli.git"
