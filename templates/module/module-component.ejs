import React, {Component, PropTypes} from 'react';
import classNames from 'classnames/bind';
// Import scss
import styles from '../<%= name %>.scss';

const cx = classNames.bind(styles);

class <%= helpers.capitalize(name) %> extends Component {
  static defaultProps = {
    demo: 'Hello World, Ni hao!'
  };

  static propTypes = {
    demo: PropTypes.string
  };

  render() {
    return (
      <div className={cx('<%= name %>-page')}>
        <h1>{this.props.demo}</h1>
        <p><%= helpers.capitalize(name) %> Page. You can review this page use the url 'http://localhost:9090/<%= name %>'.</p>
      </div>
    );
  }
}

export default <%= helpers.capitalize(name) %>;
