import React from 'react';
import {connect} from 'react-redux';
import {bindActionCreators} from 'redux';
import * as <%= name %>Action from '../action';
import <%= helpers.capitalize(name) %> from './components/<%= helpers.capitalize(name) %>';

const mapStateToProps = (state) => {
  return {
    <%= name %>: state.get('<%= name %>')
  };
};

const mapDispatchToProps = (dispatch) => {
  return {
    <%= name %>Action: bindActionCreators(<%= name %>Action, dispatch)
  };
};

export default connect(
  mapStateToProps,
  mapDispatchToProps
)(<%= helpers.capitalize(name) %>);
